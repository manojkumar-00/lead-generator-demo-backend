{
  "builds": [{ "src": "api/app.py", "use": "@vercel/python" }],
  "routes": [{ "src": "/(.*)", "dest": "api/app.py" }],
  "functions": {
    "api/app.py": {
      "includeFiles": [
        "api/excel_utils/**",
        "api/common_utils/**",
        "api/email_utils/**",
        "api/linkedin_utils/**"
      ]
    }
  }
}
